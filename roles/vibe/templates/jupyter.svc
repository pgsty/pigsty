# {{ ansible_managed }}
[Unit]
Description=Jupyter Lab
Documentation=https://jupyter.org/
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User={{ node_user }}
Group={{ node_user }}
WorkingDirectory={{ vibe_data }}
EnvironmentFile=-/etc/default/jupyter
ExecStart={{ jupyter_venv }}/bin/jupyter lab --config={{ jupyter_data }}/jupyter_config.py
Restart=always
RestartSec=5
LimitNOFILE=65535

[Install]
WantedBy=multi-user.target
